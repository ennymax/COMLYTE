<suite name="Suite">
    <test name = "Delivery Rate SetUp" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="com.Module.DeliveryRateCardSetUp.DELIVERY_RATE_CARD_SetUp"/>
        </classes>
    </test>

    <test name = "Order History" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="com.Module.OrderHistory.Downloadorderhistory"/>
            <class name="com.Module.OrderHistory.VIEW_ORDER_HISTORY"/>
        </classes>
    </test>

    <test name = "Create Order" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="com.Module.CreateOrder.CREATECUSTOMER"/>
            <class name="com.Module.CreateOrder.CREATE_ORDER_FROM_EXISTING_CUSTOMER"/>
            <class name="com.Module.CreateOrder.SEARCH_EXISTING_CUSTOMER"/>
            <class name="com.Module.CreateOrder.SELECT_DELIVERY_DETAILS"/>
            <class name="com.Module.CreateOrder.VIEW_CUSTOMER_DETAIL"/>
        </classes>
    </test>

    <test name = "DashBoard" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="com.Module.DashBoard.View_DashBoard_By_Date"/>
            <class name="com.Module.DashBoard.OrderFufilment"/>
            <class name="com.Module.DashBoard.Variance_Report_Dashboard"/>
        </classes>
    </test>
</suite>
